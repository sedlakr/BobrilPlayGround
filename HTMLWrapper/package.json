{
  "name": "html wrapper example",
  "version": "1.0.0",
  "description": "client for chat app",
  "main": "index.tsx",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bobril": "*",
    "typescript": "*"
  },
  "devDependencies": {},
  "bobril": {
    "title": "Bobril html wrapper example",
    "head": "<meta name=\"format-detection\"><style>html, body {width: 100%; height: 100%; overflow: hidden}</style><meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><meta name=\"msapplication-config\" content=\"none\">",
    "dir": "build",
    "bbVersion": "1.22.0",
    "compilerOptions": {
      "declaration": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "noEmitHelpers": true,
      "preserveConstEnums": false,
      "removeComments": false,
      "skipLibCheck": true,
      "skipDefaultLibCheck": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true
    }
  }
}
